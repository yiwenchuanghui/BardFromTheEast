    "react-player": "^0.25.3",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1"
  },
  "author": "Dastan Zhanibek",
  "bugs": {
    "url": "https://https://github.com/yiwenchuanghui/BardFromTheEast/issues"
