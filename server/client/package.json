    "preview": "rm -rf ./build && NODE_ENV=\"production\" webpack-dashboard -t 'Preview Mode - BFE' -- webpack-dev-server",
    "hook-add": "prepush install",
    "hook-remove": "prepush remove"
  },
  "keywords": [
    "react",
    "redux",
    "webpack2"
  ],
  "devDependencies": {
    "autoprefixer": "^6.5.3",
